      // Fasting

      "_monthViewHeadline": true,
      "rules": {
        "fast.none": {
          "summary": "No Fast",
          "_visibilityIncludeDates": [ "Wednesday", "Friday" ]
        },
        "fast.dairy": {
          "summary": "Fast üßÄüêüüè∫üç∑",
          "elaboration": "Dairy, Fish, Oil, and Wine Permitted"
        },
        "fast.fish": {
          "summary": "Fast üêüüè∫üç∑",
          "elaboration": "Fish, Oil, and Wine Permitted"
        },
        "fast.caviar": {
          "summary": "Fast ü•Ñüè∫üç∑",
          "elaboration": "Caviar, Oil, and Wine Permitted"
        },
        "fast.oil": {
          "summary": "Fast üè∫üç∑",
          "elaboration": "Oil and Wine Permitted"
        },
        "fast.wine": {
          "summary": "Fast üç∑",
          "elaboration": "Wine Permitted"
        },
        "fast.strict": {
          "summary": "Fast",
          "elaboration": "Strict Fast"
        }
      }
    }
  },

  //###########################################################################
  // Event Ranks

  "eventRanks": {
    "great.feast": {
      "precedence": 1,
      "_monthViewContent": true
    },
    "patron": {
      "precedence": 2,
      "_monthViewContent": true
    },
    "vigil": {
      "precedence": 3,
      "_monthViewContent": true
    },
    "polyeleos": {
      "precedence": 4
    },
    "doxology": {
      "precedence": 5
    },
    "six.stichera": {
      "precedence": 6
    },
    "named.day": {
      "precedence": 99,
      "_monthViewContent": true
    }
  },

  //###########################################################################
  // Seasons

  "seasons": {

    //---------------------------------------------------------------------------
    // Ordinary Time

    "ordinary": {
      "startDate": "1/1",
      "endDate": "12/31",
      "isDefault": true,
      "ruleCriteria": {
        "fast": [
          {
            "includeDates": [ "Wednesday", "Friday" ],
            "ruleKey": "fast.strict"
          },
          {
            "includeDates": [ "Wednesday", "Friday" ],
            "includeRanks": [ "great.feast", "patron", "vigil" ],
            "ruleKey": "fast.fish"
          }
        ]
      }
    },

    //---------------------------------------------------------------------------
    // Advent

    "advent": {
      "startDate": "11/15",
      "endDate": "12/24",
      "ruleCriteria": {
        "fast": [
          {
            "endDate": "12/19", // except nativity forefeast
            "includeDates": [ "Sunday", "Saturday" ],
            "ruleKey": "fast.fish"
          },
          {
            "startDate": "12/20", // nativity forefeast
            "includeDates": [ "Sunday", "Saturday" ],
            "ruleKey": "fast.oil"
          },
          {
            "includeDates": [ "Monday", "Wednesday", "Friday" ],
            "ruleKey": "fast.strict"
          },
          {
            "includeDates": [ "Tuesday", "Thursday" ],
            "ruleKey": "fast.oil"
          },
          {
            "endDate": "12/19", // except nativity forefeast
            "includeRanks": [ "great.feast", "patron", "vigil" ],
            "ruleKey": "fast.fish"
          },
          {
            "endDate": "12/19", // except nativity forefeast
            "includeRanks": [ "polyeleos", "doxology" ],
            "includeDates": [ "Monday", "Tuesday", "Thursday" ],
            "ruleKey": "fast.fish"
          }
        ]
      }
    },

    //---------------------------------------------------------------------------
    // Christmas

    "christmas": {
      "startDate": "12/25",
      "endDate": "1/5",
      "ruleCriteria": {
        "fast": [
          {
            "ruleKey": "fast.none"
          }
        ]
      }
    },

    //---------------------------------------------------------------------------
    // Week of the Publican & Pharisee

    "publican.pharisee": {
      "startDate": "-10/Sunday",
      "endDate": "-10/Saturday",
      "ruleCriteria": {
        "fast": [
          {
            "ruleKey": "fast.none"
          }
        ]
      }
    },

    //---------------------------------------------------------------------------
    // Cheesefare Week

    "cheesefare": {
      "startDate": "-8/Monday",
      "endDate": "-7/Sunday",
      "ruleCriteria": {
        "fast": [
          {
            "ruleKey": "fast.dairy"
          }
        ]
      }
    },

    //---------------------------------------------------------------------------
    // Lent

    "lent": {
      "startDate": "-7/Monday",
      "endDate": "-2/Saturday",
      "ruleCriteria": {
        "fast": [
          {
            "endDate": "-7/Friday", // Canon of St Andrew
            "ruleKey": "fast.strict"
          },
          {
            "startDate": "-7/Saturday", // after Canon of St Andrew
            "includeDates": [ "Sunday", "Saturday" ],
            "excludeDates": [ "-2/Saturday" ], // except Lazarus Saturday
            "ruleKey": "fast.oil"
          },
          {
            "includeDates": [ "-2/Saturday" ], // Lazarus Saturday
            "ruleKey": "fast.caviar"
          },
          {
            "startDate": "-7/Saturday", // after Canon of St Andrew
            "excludeDates": [ "Sunday", "Saturday" ],
            "ruleKey": "fast.strict"
          },
          {
            "startDate": "-7/Saturday", // after Canon of St Andrew
            "includeRanks": [ "great.feast", "patron" ],
            "ruleKey": "fast.fish"
          },
          {
            "startDate": "-7/Saturday", // after Canon of St Andrew
            "includeRanks": [ "vigil", "polyeleos" ],
            "ruleKey": "fast.oil"
          }
        ]
      }
    },

    //---------------------------------------------------------------------------
    // Holy Week

    "holy.week": {
      "startDate": "-1/Sunday",
      "endDate": "-1/Saturday",
      "ruleCriteria": {
        "fast": [
          {
            "endDate": "-1/Friday", // through Holy Friday
            "ruleKey": "fast.strict"
          },
          {
            "includeDates": [ "-1/Saturday" ], // Holy Saturday
            "ruleKey": "fast.wine"
          },
          {
            "endDate": "-1/Thursday", // through Holy Thursday
            "includeRanks": [ "great.feast" ],
            "ruleKey": "fast.oil"
          },
          {
            "startDate": "-1/Friday", // starting at Holy Friday
            "includeRanks": [ "great.feast" ],
            "ruleKey": "fast.wine"
          }
        ]
      }
    },

    //---------------------------------------------------------------------------
    // Pascha

    "pascha": {
      "startDate": "1/Sunday",
      "endDate": "7/Saturday",
      "ruleCriteria": {
        "fast": [
          {
            "endDate": "1/Saturday", // Bright Week
            "ruleKey": "fast.none"
          },
          {
            "startDate": "2/Sunday", // after Bright Week
            "includeDates": [ "Wednesday", "Friday" ],
            "ruleKey": "fast.strict"
          },
          {
            "startDate": "2/Sunday", // after Bright Week
            "includeDates": [ "Wednesday", "Friday" ],
            "includeRanks": [ "great.feast", "patron", "vigil" ],
            "ruleKey": "fast.fish"
          }
        ]
      }
    },

    //---------------------------------------------------------------------------
    // Pentecost Week

    "pentecost": {
      "startDate": "8/Sunday",
      "endDate": "8/Saturday",
      "ruleCriteria": {
        "fast": [
          {
            "ruleKey": "fast.none"
          }
        ]
      }
    },

    //---------------------------------------------------------------------------
    // Apostles Fast

    "peter.paul": {
      "startDate": "9/Monday",
      "endDate": "6/28",
      "ruleCriteria": {
        "fast": [
          {
            "includeDates": [ "Sunday", "Saturday" ],
            "ruleKey": "fast.fish"
          },
          {
            "includeDates": [ "Monday", "Wednesday", "Friday" ],
            "ruleKey": "fast.strict"
          },
          {
            "includeDates": [ "Tuesday", "Thursday" ],
            "ruleKey": "fast.oil"
          },
          {
            "includeRanks": [ "great.feast", "patron", "vigil" ],
            "ruleKey": "fast.fish"
          },
          {
            "includeRanks": [ "polyeleos", "doxology" ],
            "includeDates": [ "Monday", "Tuesday", "Thursday" ],
            "ruleKey": "fast.fish"
          }
        ]
      }
    },

    //---------------------------------------------------------------------------
    // Dormition Fast

    "dormition.theotokos": {
      "startDate": "8/1",
      "endDate": "8/14",
      "ruleCriteria": {
        "fast": [
          {
            "includeDates": [ "Sunday", "Saturday" ],
            "ruleKey": "fast.oil"
          },
          {
            "excludeDates": [ "Sunday", "Saturday" ],
            "ruleKey": "fast.strict"
          },
          {
            "includeRanks": [ "great.feast", "patron" ],
            "ruleKey": "fast.fish"
          },
          {
            "startDate": "-7/Saturday",
            "includeRanks": [ "vigil", "polyeleos" ],
            "ruleKey": "fast.oil"
          }
        ]
      }
    }
  },

